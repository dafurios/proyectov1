<h1>Mi Datos de Tutor</h1>

<table>
  <thead>
    <tr>
      <th>Foto</th>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Apellido Paterno</th>
      <th>Apellido Materno</th>
      <th>Dirección</th>
      <th>Teléfono</th>
      <th>coordinador</th>
      <th>Nivel</th>
      <th>Curso</th>
      <th>Nombre Apoderado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= image_tag @tutor.image.thumb %></td>
        <td><%= @tutor.rut %></td>
        <td><%= @tutor.name %></td>
        <td><%= @tutor.lastname %></td>
        <td><%= @tutor.surname %></td>
        <td><%= @tutor.address %></td>
        <td><%= @tutor.phone %></td>
        <td><%= @tutor.school %></td>
        <td><%= @tutor.level %></td>
        <td><%= @tutor.course %></td>
        <td><%= @tutor.name %></td>
        <td><%= link_to 'Show', tutor_path(current_user) %></td>
        <td><%= link_to 'Editar', edit_tutor_path(current_user) %></td>
      </tr>

  </tbody>
</table>

<br>
